        "unplugin-vue-components": "^0.24.0",
        "vite": "^4.1.2",
        "vite-plugin-eslint": "^1.8.1",
        "vite-ssg": "^0.22.1",
        "vitest": "^0.29.8",
        "vue-tsc": "^1.1.3"
    },
    "license": "MIT",
    "lint-staged": {
        "src/**/*.{js,ts,tsx,jsx,vue}": [
            "eslint --fix",
            "git add"
